{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "ignoreImports": "off",
    "ignoreDestructuring": "off",
    "camelcase": "off",
    "react/prop-types": "off",
    "padded-blocks": "off",
    "semi": "off",
    "singleQuote": false
  }
}
